<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="nls1729-extensions">
  <schema path="/org/gnome/shell/extensions/activities-config/" id="org.gnome.shell.extensions.activities-config">
    <key type="s" name="original-activities-button-text">
      <default>""</default>
      <summary>Original Activities Button Text.</summary>
      <description>If not empty, contains the original text displayed for the Activities Button.</description>
    </key>
    <key type="b" name="activities-config-button-removed">
      <default>false</default>
      <summary>Disable all Activities Button functions.</summary>
      <description>The Activities Button is removed.</description>
    </key>
    <key type="s" name="activities-config-button-text">
      <default>""</default>
      <summary>Activities Button Text.</summary>
      <description>If not empty, it contains the custom text displayed for the Activities Button.</description>
    </key>
    <key type="i" name="activities-text-padding">
      <default>4</default>
      <summary>Text padding in pixels.</summary>
      <description>Horizontal padding for Activities Text.</description>
    </key>
    <key type="b" name="activities-config-button-no-text">
      <default>false</default>
      <summary>Disable Activities Button Text.</summary>
      <description>The Activities Button Text is not displayed if true.</description>
    </key>
    <key type="s" name="activities-config-button-icon-path">
      <default>"/usr/share/icons/gnome/24x24/status/info.png"</default>
      <summary>Activities Button Icon Path.</summary>
      <description>If not empty, it contains the path of an icon displayed with the Activities Button Text.</description>
    </key>
    <key type="i" name="activities-icon-padding">
      <default>4</default>
      <summary>Icon padding in pixels.</summary>
      <description>Horizontal padding for Activities Icon.</description>
    </key>
    <key type="b" name="activities-config-button-no-icon">
      <default>false</default>
      <summary>Disable Activities Button Icon.</summary>
      <description>The Activities Button Icon is not displayed if true.</description>
    </key>
    <key type="i" name="activities-config-hot-corner-delay">
      <default>250</default>
      <summary>Icon padding in pixels.</summary>
      <description>Horizontal padding for Activities Icon.</description>
    </key>
    <key type="b" name="activities-config-hot-corner">
      <default>false</default>
      <summary>Disable Activities Hot Corner.</summary>
      <description>The Activities Hot Corner is disabled if true.</description>
    </key>
    <key type="b" name="transparent-panel">
      <default>false</default>
      <summary>Transparent Panel.</summary>
      <description>Set the Panel to transparent.</description>
    </key>
  </schema>
</schemalist>